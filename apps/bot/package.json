{
  "name": "@elvia/bot",
  "version": "7.0.0-beta",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsup",
    "start": "node --no-warnings ./dist/index.js",
    "start:shard": "node --no-warnings ./dist/shard.js",
    "lint": "eslint \"src/**/*.ts\" --format=pretty",
    "lint:fix": "eslint \"src/**/*.ts\" --fix --format=pretty",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "typecheck": "tsc --noEmit",
    "register": "tsx --no-warnings ./scripts/register.ts"
  },
  "license": "GPL-3.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/rygent/Elvia.git",
    "directory": "apps/bot"
  },
  "dependencies": {
    "@elvia/core": "workspace:^",
    "@elvia/database": "workspace:^",
    "@elvia/logger": "workspace:^",
    "@elvia/utils": "workspace:^",
    "@iamtraction/google-translate": "^2.0.1",
    "@rygent/anilist": "^1.5.0",
    "@rygent/spotify": "^1.2.1",
    "@sapphire/discord-utilities": "^3.5.0",
    "@sapphire/time-utilities": "^1.7.14",
    "@sapphire/type": "^2.6.0",
    "@sapphire/utilities": "^3.18.2",
    "colorette": "^2.0.20",
    "discord-api-types": "^0.38.37",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "globby": "^15.0.0",
    "jiti": "^2.6.1",
    "moment": "^2.30.1",
    "moment-duration-format": "^2.3.2",
    "nanoid": "^5.1.6",
    "systeminformation": "^5.28.7",
    "undici": "^7.16.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@swc/core": "^1.15.8",
    "@types/moment-duration-format": "^2.2.7",
    "@types/node": "^24.10.4",
    "eslint": "^9.39.2",
    "eslint-formatter-pretty": "^7.0.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "optionalDependencies": {
    "bufferutil": "^4.1.0"
  },
  "packageManager": "pnpm@10.26.1"
}
