{
  "name": "@elvia/bot",
  "version": "5.6.0",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsx ./scripts/clean.ts && pnpm run build:swc",
    "build:swc": "swc ./src --out-dir ./dist --copy-files --strip-leading-paths",
    "start": "node --no-warnings ./dist/index.js",
    "start:shard": "node --no-warnings ./dist/shard.js",
    "lint": "eslint \"src/**/*.ts\" --format=pretty",
    "lint:fix": "eslint \"src/**/*.ts\" --fix --format=pretty",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "typecheck": "tsc --noEmit",
    "register": "tsx --no-warnings ./scripts/register.ts"
  },
  "license": "GPL-3.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/rygent/Elvia.git",
    "directory": "apps/bot"
  },
  "dependencies": {
    "@elvia/database": "workspace:^",
    "@elvia/logger": "workspace:^",
    "@elvia/webserver": "workspace:^",
    "@iamtraction/google-translate": "^2.0.1",
    "@prisma/client": "^6.5.0",
    "@rygent/anilist": "^1.4.0",
    "@rygent/spotify": "^1.2.0",
    "@sapphire/discord-utilities": "^3.4.4",
    "@sapphire/time-utilities": "^1.7.14",
    "@sapphire/type": "^2.6.0",
    "@sapphire/utilities": "^3.18.2",
    "axios": "^1.8.3",
    "colorette": "^2.0.20",
    "discord-api-types": "0.37.119",
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.7",
    "express": "^5.0.1",
    "globby": "^14.1.0",
    "http-errors": "^2.0.0",
    "jiti": "^2.4.2",
    "moment": "^2.30.1",
    "moment-duration-format": "^2.3.2",
    "nanoid": "^5.1.5",
    "systeminformation": "^5.25.11",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.9",
    "@types/moment-duration-format": "^2.2.6",
    "@types/node": "^22.13.10",
    "commander": "^13.1.0",
    "eslint": "^9.22.0",
    "eslint-formatter-pretty": "^6.0.1",
    "prettier": "^3.5.3",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9"
  },
  "engines": {
    "node": ">=v22.12.0"
  },
  "packageManager": "pnpm@10.6.2"
}
