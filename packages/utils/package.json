  "devDependencies": {
    "@types/node": "^22.14.0",
    "eslint": "^9.24.0",
    "eslint-formatter-pretty": "^6.0.1",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "files": [
    "./dist/"
  ],
  "engines": {
    "node": ">=v22.12.0"
  },
